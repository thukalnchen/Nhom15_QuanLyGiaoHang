# Environment Variables
NODE_ENV=development
PORT=3000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=food_delivery_db
DB_USER=postgres
DB_PASSWORD=your_password_here

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# CORS Configuration (leave empty to allow all localhost origins in development)
CORS_ORIGIN=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
# Set DISABLE_RATE_LIMIT=true to completely disable rate limiting in development (for testing multiple clients)
DISABLE_RATE_LIMIT=false

